{
  "name": "moneybin",
  "version": "0.0.1",
  "description": "",
  "main": "public/electron.js",
  "build": {
    "appId": "com.electron.moneybin",
    "mac": {
      "category": "public.app-category.finance"
    }
  },
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test --env=jsdom",
    "electron-build": "CSC_IDENTITY_AUTO_DISCOVERY=false build --mac --win",
    "electron-test": "jest public/",
    "start": "cross-env BROWSER=none NODE_ENV=local concurrently \"npm:react-start\" \"wait-on http://localhost:3000 && electron .\"",
    "debug": "cross-env BROWSER=none NODE_ENV=local concurrently \"npm:react-start\" \"wait-on http://localhost:3000 && electron . --debug\"",
    "build": "react-build && electron-build",
    "postinstall": "electron-builder install-app-deps"
  },
  "author": "",
  "license": "ISC",
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^10.4.9",
    "@testing-library/user-event": "^12.1.3",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "electron": "^10.1.0",
    "electron-builder": "^22.8.0",
    "wait-on": "^5.2.0"
  },
  "dependencies": {
    "d3": "^6.0.0",
    "d3-sankey-circular": "^0.34.0",
    "history": "^5.0.0",
    "lodash": "^4.17.20",
    "moment": "^2.27.0",
    "purecss": "^2.0.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-modal": "^3.11.2",
    "react-scripts": "^3.4.3",
    "react-uid": "^2.3.0",
    "sqlite3": "^5.0.0",
    "winston": "^3.3.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
